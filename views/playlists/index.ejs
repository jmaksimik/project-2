<%- include('../partials/header')%> 
    <h1>My Playlists</h1>
    <button id="adds"><a href="/playlists/new">Add Playlist</a></button>
    <ul>
        <% playlist.forEach(p => { %>
            <li><a href='/playlists/<%=p._id%>'><%= p.playlistName%></a>
            <form class = "delete-form" action="/playlists/<%=p._id%>?_method=DELETE"
                class="my-playlist-delete" method="POST">
                    <button id="deletes" type="submit">X</button>
                </form>
            </li>
        <% }) %>
    </ul>
<%- include('../partials/footer')%> 